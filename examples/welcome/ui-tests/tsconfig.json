{
  "compilerOptions": {
    "lib": [
      "ESNext"
    ],
    "outDir": "./dist",
    "target": "ESNext",
    "skipLibCheck": true,
    "allowJs": false,
    "alwaysStrict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "incremental": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "noFallthroughCasesInSwitch": true,
    "strictPropertyInitialization": false,
    "baseUrl": ".",
    "rootDirs": [
      "src"
    ],
    "paths": {
      "pages/*": [
        "src/pages/*"
      ],
      "setup/*": [
        "src/setup/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "*.ts",
    ".eslintrc.cjs"
  ],
  "exclude": [
    "node_modules",
  ]
}
